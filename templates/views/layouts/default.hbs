			jQuery.scrollDepth({
			  elements: ['#caring', '#maintain', '#sense', '#fix'],
			  percentage: false,
			});



			var height = $('.header').height();
				var nav = $("#nav");
				$(window).bind("scroll", function(e) {
				    //console.log('body', $('body').scrollTop());
				  if ($('body').scrollTop() > height-110) {
				    nav.addClass("fix");
				  }
				  else {
				    nav.removeClass("fix");
				  }
				  
				});


		</script>
